@import "compass/css3";
@import url(https://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800);
$blue:#297cc1;
$light_blue:#b8edff;

body{
    background: #222;
    color:#aaa;
    font-size:14px;
    font-family: 'Open Sans', sans-serif;
}

.logo{
	margin-top: 10px;
	margin-bottom: 5px;
	max-width:240px;
}

.hero{	
	padding: 0 !important;
}

.fltrgt{
    float: right;
}

.billboard{
	/*style="background-image:url('https://image.tmdb.org/t/p/original/{{hero.poster_path}}');"*/
	background: url(../img/hero.jpg) no-repeat 0 0;
	background-position: center center;
	// background-size:cover;
	min-height: 600px;
	margin-bottom: 30px;
	border-top: 1px solid #111;
	border-bottom:1px solid #666;

}

h1.heading{
	font-size: 30px;
	text-transform: uppercase;
}

.col-sm-12.heading_col {
    height: 70px;
}

.form-group.select{
	margin-bottom: 0;
}

select{
	position:relative;
	bottom:30px;
}

.heading_col{
	margin-bottom: 0;
}

input.btn{
	background: $blue !important;
	color: #fff;
}

p.error{
	color: tomato;
}

.header{
	background:#141414;
	.container{
		position:relative;
	}
}

.movies{
	border-bottom:1px solid #555;
	padding-bottom: 20px;
	margin-bottom: 50px;
}

#searchMovieForm{
	margin-top: 30px;
	margin-bottom: 7px;
	
	.form-group{
		margin-bottom: 0;
	}

	.form-holder{
		margin-bottom: 0;
		border: 1px solid darken(#3f3f3f,6%);
		background: #000;
		height: 42px;
		margin-bottom: 10px;
		@include border-radius(0 5px 5px 0);
	}
	
	input.form-control{
		background: #000;
		border: none;
		float: left;
		width: 75%;
		height: 40px;
		color:#ddd;
	}

	input.btn{
		float: left;
		width: 25%;
		height: 40px;
		@include border-radius(0 5px 5px 0);

		&:hover,&:active{
			color:#fff;
			background-color: lighten($blue,5%) !important;
		}
	}
}

.ng-invalid-required.ng-submitted .form-holder{
	border:1px solid tomato !important;
}

#results{
	background: #303030;
	padding: 20px 10px;
	padding-bottom: 0;
	position:absolute;
	z-index:99999;
	width:100%;

	h2{
		margin-bottom: 20px;
		margin-left: 20px;
	}
}

.closeBtn{
	color:#ddd;
	position: absolute;
	right: 5px;
	top: 0;

	&:hover{
		color:darken(#ddd, 20%);
	}
}

.bg_container{
	padding: 0;
	background-attachment: fixed;
	min-height:500px;
	background-size:cover;
	background-position: top center;
	background-repeat:no-repeat;
}

.bg{
    position: fixed;
    @include opacity(0.3);
    max-width: 100%;
}

.result_image img, .seasons img{
	margin-bottom: 5px;
	border:1px solid #222;
	&:hover{
		@include box-shadow(0 3px 5px #111);
		border: 1px solid #666;
	}
}

.tv_details .seasons{

	img{
		margin-top: 15px;
	}

	p{
		text-align: center;
		margin-bottom: 3px;
	}
}

.poster{
	margin-bottom: 50px;
	border:3px solid #888;
	position:relative;
	bottom:140px;
}

.poster.poster_orgpos{
	bottom:0;
}

.actor{
	border-top:1px solid #333;
	padding-top: 30px;

	p.overview{
		line-height:1.8em;
		margin-top: 30px;
		margin-bottom: 45px;
	}

	.preview_poster{
		min-height:185px;
	}
}

.colorbg{
	background:#141414;
	padding: 20px;
	overflow:auto;
	margin-bottom: 30px;
}

.actor_details .colorbg{
	margin-top: 30px;
	padding-bottom: 30px;
}

.featuredin{
	margin-top: 20px;
	border-top:1px solid #444;
	padding-top: 40px;
	overflow:auto;
	
	&.nobdr{
		margin-top: 0;
		padding-top: 25px;
		border-top:none;
	}
}

iframe{
	margin-top: 20px;
}

.year_aired{
	margin-bottom: 30px;
	font-style:italic;
	font-weight: 500;
}

h2.title{
	font-weight: 800;
	margin-top: 15px;
	margin-bottom: 3px;
	font-size: 30px;
	text-transform:uppercase;
	color:#ddd;
	
	span.episode_name{
		text-transform:none;
	}	
}

h3.section_heading{
	margin-bottom: 20px;
	margin-left: 15px;
	font-weight: 800;
	font-size: 22px;
	color:$light_blue;
}

.embed-responsive{
	margin-top: 30px;
	margin-bottom: 70px;
}

.result_title{
	background:rgba(0,0,0,0.8);
	background:$blue;
	position:relative;
	bottom:67px;
	padding-bottom: 15px;
}

.result_main{
	position:relative;
	bottom:67px;
	background:#222;

	.list-group{
		position:relative;
		bottom:140px;
	}
}

#overview.movie_details{
	margin-bottom: 0px;
}
.movie_details, .tv_details{
	// background: rgba(0,0,0,0.8);
	margin-top: 40px;
	margin-bottom: 40px;
	overflow:auto;

	h2.title{
		margin-top: 15px;
	}

	p{
		margin-bottom: 7px;

		&.genres{
			margin-bottom: 30px;
		}

		&.overview{
			margin-bottom: 30px;
			line-height:2.0em;
		}

		.plabel{
			color:$light_blue
		}
	}
	
	.rating{
		margin-bottom: 20px;
		background:$blue;
		color:#fff;
		padding:2px;
		max-width:55px;
		font-size: 15px;

		p{margin-bottom: 0;text-align: center;}
	}

	.movie_images img{
		margin: 10px;
	}

	.castmember, .seasons{
		min-height: 360px;

		img{
			margin-bottom: 5px;
			margin-top: 10px;
			border:1px solid #222;

			&:hover{
				border: 1px solid #666;
			}
		}

		p{
			margin-bottom: 5px;
			text-align: center;
			b{
				color:$blue;
			}
		}
	}
}

.actor_details{
	p{
		margin-bottom: 7px;
		.plabel{
			color:$light_blue;	
		}
	}
}

.season_overview{
	p.overview{
		// margin-bottom: 50px;
	}
}


h3.tagline{
	font-style:italic;
	font-size:17px;
	color:$light_blue;
}

.recommended{
	border-top:1px solid #444;
	padding-top: 50px;
	clear:both;
}

.episodes{
	margin-top: 20px;
	margin-bottom: 30px;
	border-bottom:1px solid #555;
	padding-bottom: 10px;
	
	.name{
		font-size: 21px;
		font-weight: 700;

		span{
			color:$blue;
			font-weight: 700;
		}
	}
	.air_date{
		font-size: 13px;
		font-style:italic;
		margin-bottom: 20px;
	}

	span{
		font-weight: 500;
		font-style:italic;
	}
}

.episode_thumbnail{
	margin:0 auto;
	margin-bottom: 15px;
	display:block;
	border:1px solid #222;
	&:hover{
		border:1px solid #555;
	}
}

.season_episode{
	margin-bottom: 10px;
}

select{
	color:#333;
}
.actor_gallery{
	margin-bottom: 20px;
}

@media handheld, only screen and (min-width: 1200px) { 
	.preview_poster{
		min-height:276px;
		margin-bottom: 10px;
	}
}

@media handheld, only screen and (max-width: 1200px) { 
	.preview_poster{
		margin-bottom: 25px;
	}

	.actor .preview_poster{
		min-height:140px;
	}

	.movie_details .castmember, .movie_details .seasons, .tv_details .castmember, .tv_details .seasons{
		min-height:310px;
	}
}

@media handheld, only screen and (max-width: 991px) { 

	.movie_details .castmember, .movie_details .seasons, .tv_details .castmember, .tv_details .seasons{
		min-height:250px;
	}
	.actor .preview_poster {
	    min-height: 80px;
	}

	.bg_container, .billboard{
		min-height:420px;
		background-attachment:scroll;
	}

	.billboard{
		background-size:200%;
	}

	.poster{
		margin-top: 0;
	}
}

@media handheld, only screen and (max-width: 768px) { 
	.movie_details .movie_images img, .tv_details .movie_images img{
		margin-left: 0;margin-right: 0;
	}
	.logo{
		margin: 0 auto;
		margin-top: 15px;
	}

	#searchMovieForm{margin-top: 20px;margin-bottom: 20px;}

	.movie_details .castmember, .movie_details .seasons, .tv_details .castmember, .tv_details .seasons{
		min-height:280px;
	}

	.movie_details .castmember{
		min-height:310px;
	}

	.result_main .list-group {
	    bottom: 0px; 
	    margin-top: 40px;
	}

	.bg_container, .billboard{
		min-height:400px;
		background-attachment:scroll;
	}

	.billboard{
		background-size:185%;
	}

	h2.title{
		font-size: 27px;
	}

	h3.tagline{
		font-size: 16px;
	}

	.movie_details, .result_main,.nopadmob{
		// border:1px solid red;
		padding: 0;
	}

	.episodes{
		padding: 0 15px;
		p{text-align: center;}
	}

}

@media handheld, only screen and (max-width: 630px) { 
	.billboard{
		background-size:175%;
	}

	.bg_container, .billboard{
		min-height:330px;
	}

	.episodes .name{font-size: 17px;}
}

@media handheld, only screen and (max-width: 498px) { 

	.billboard{
		background-size:165%;
	}

	.bg_container, .billboard{
		min-height:230px;
	}

	.logo{
		max-width:185px;
	}

	#searchMovieForm{
		margin-top: 10px;
	}

	.episodes .name{font-size: 16px;}
}


@media handheld, only screen and (max-width: 320px) { 
	.billboard{
		background-size:175%;
	}

	.bg_container, .billboard{
		min-height:210px;
	}
}


